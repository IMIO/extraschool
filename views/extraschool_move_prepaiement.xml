<?xml version="1.0" ?>
<odoo>
	<data>
        <record model="ir.ui.view" id="extraschool_move_prepaiement_form">
            <field name="name">extraschool.move_prepaiement.wizard.form</field>
            <field name="model">extraschool.move_prepaiement</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Move prepaiment">
                    <group>
                        <field name="categ_id" domain="[('id', '!=', default_activity_category_id)]"/>
                        <field name="amount"/>
                    	<button name="move" string="Move" icon="gtk-apply" type="object"/>
                    </group>
                </form>
            </field>
        </record>


	</data>


</odoo>
