<?xml version="1.0" ?>
<openerp>
	<data>        
		<record model="ir.ui.view" id="extraschool_child_tree">
			<field name="name">Child</field>
			<field name="model">extraschool.child</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Child">					
                    <field name="lastname"/>
                    <field name="firstname"/> 
                    <field name="schoolimplantation"/>            
				</tree>
			</field>
		</record>
		<record model="ir.ui.view" id="extraschool_child_form">
			<field name="name">Child</field>
			<field name="model">extraschool.child</field>            
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Child">
				<group col="4">
				    <field name="isdisabled"/>
                    <newline/>        
                    <field name="name"  invisible="True"/>            
                    <field name="lastname" on_change="onchange_name(lastname,firstname)"/>            
					<field name="firstname" on_change="onchange_name(lastname,firstname)"/>
                    <newline/>
                    <field name="schoolimplantation"/>					
                    <newline/>
                    <field name="childtypeid"/>
                    <newline/>
                    <field name="levelid"/>
                    <newline/>
                    <field name="classid"/>
                    <field name="birthdate"/>
                    <field name="tagid"/>
                    <button name="action_gentagid" type="object" string="Generate TagID" />
                    <newline/>
                    <field name="otherref"/>
                    <newline/>
                    <field name="parentid"/>
                    <newline/>
                    <button name="return_action_to_open" type="object" context="{'xml_id':'action_lbsc_childsworkbook_wizard'}" string="Workbook sheet" />
                    </group>
				</form>
			</field>
		</record>        
	</data>
</openerp>
