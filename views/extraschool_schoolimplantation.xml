<?xml version="1.0" ?>
<odoo>
	<data>        
        <record model="ir.ui.view" id="extraschool_schoolimplantation_tree">
			<field name="name">extraschool.schoolimplantation.tree</field>
			<field name="model">extraschool.schoolimplantation</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="School Implantation">
                    <field name="name"/> 
				</tree>
			</field>
		</record>
        <record model="ir.ui.view" id="extraschool_schoolimplantation_form">
            <field name="name">extraschool.schoolimplantation.form</field>
            <field name="model">extraschool.schoolimplantation</field>    
            <field name="type">form</field>
            <field name="arch" type="xml">
            <form string="School Implantation"> 
            <group col="4">
                <field name="schoolid"/>
                <newline/>
                <field name="name"/>
                <newline/>
                <field name="street"/> 
                <newline/>
                <field name="zipcode"/>        
                <newline/>
                <field name="city"/>
            </group>
            </form>
            </field> 
        </record>        
	</data>
</odoo>
