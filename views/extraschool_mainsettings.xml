<?xml version="1.0" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="extraschool_mainsettings_form">
            <field name="name">mainsettings.form</field>
            <field name="model">extraschool.mainsettings</field>
            <!-- field name="priority" eval="17"/ -->
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Main Settings">
                    <sheet>
                        <div class="alert alert-warning" attrs="{'invisible':[('upgrade_level_ready', '=', False)]}">
                            Il n'y a pas de facturier en Juin :(
                        </div>
                        <notebook>
                            <page string="Update Prestation Statistic">
                                <button name="update_presta_stat" string="Update presta stat" icon="gtk-apply"
                                        type="object"/>
                            </page>
                            <page string="Fiche parent">
                                <group string="Mettre en majuscule l'adresse du parent" col="2">
                                    <button
                                        name="put_upper_address"
                                        string="Mettre adresses en majuscule"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                </group>
                                <group string="Enlever les virgules dans les adresses" col="2">
                                    <button
                                        name="virgule_address"
                                        string="Enlever les virgules dans les adresses"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                </group>
                                <group string="RN pour l'import ONYX" col="2">
                                    <button
                                        name="put_rn_onyx"
                                        string="Mettre le RN pour l'import ONYX"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                </group>
                            </page>
                            <page string="Fiche Enfant">
                                <group string="RN pour l'import ONYX" col="2">
                                    <button
                                        name="put_rn_onyx_child"
                                        string="Mettre le RN pour l'import ONYX"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                </group>
                            </page>
                            <page string="Re check POD - Error when building biller">
                                <group col="2">
                                    <field name="date_from"/>
                                    <field name="date_to"/>
                                </group>
                                <button name="re_check_pod" string="Check" icon="gtk-apply" type="object"/>
                                <button name="reset_check" string="Reset AND Check" icon="gtk-apply" type="object"/>
                            </page>
                            <page string="Upgrade Child Levels">
                                <group col="4">
                                    <group string="Upgrade Children" col="2">
                                        <field name="upgrade_level_ready" invisible="True"/>
                                        <field name="levelbeforedisable"/>
                                        <field name="last_level_id"/>
                                        <field name="date_child_upgrade"/>
                                        <field name="last_child_upgrade_levels"/>
                                        <newline/>
                                        <button
                                            name="childupgradelevels"
                                            string="Update level"
                                            icon="gtk-apply"
                                            type="object"
                                        />
                                    </group>
                                    <group string="Revert Upgrade" col="2">
                                        <field name="date_revert_upgrade"/>
                                        <button name="revert_upgrade" string="Revert Update level" icon="gtk-apply"
                                                type="object"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Bouton pour Coralie &lt;3">
                                <group string="Supprimer présences" col="2">
                                    <group>
                                        <field name="limit_date_delete_presta"/>
                                        <button
                                            name="delete_presta_test"
                                            string="Supprimer les données des présences"
                                            icon="gtk-apply"
                                            type="object"
                                            confirm="Es-tu sûre Coralie ? Cette action est irréversible !"
                                        />
                                    </group>
                                    <group>
                                        <field name="pdf_true_biller"/>
                                        <field name="pdf_true_tax"/>
                                        <button
                                            name="pdf_to_true"
                                            string="Mettre PDF à True"
                                            icon="gtk-apply"
                                            type="object"
                                            confirm="Es-tu sûre Coralie ? Cette action est irréversible !"
                                        />
                                    </group>
                                </group>
                                <group string="Affecter classe" col="2">
                                    <button
                                        name="mettet_grandprimaire"
                                        string="Grand primaire / Petit primaire"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                </group>
                                <group string="Affecter catégorie tarifaire" col="2">
                                    <field name="level"/>
                                    <field name="type_child"/>
                                    <button
                                        name="type_grandprimaire"
                                        string="Grand primaire / Petit primaire"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                </group>
                                <group string="Spécial MSG">
                                    <button
                                        name="delete_age_group"
                                        string="Supprimer les groupes d'âges = 0 - 0"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                    <button
                                        name="delete_doctors"
                                        string="Supprimer les médecins"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                    <button
                                        name="delete_allergies"
                                        string="Supprimer les allergies"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                    <button
                                        name="delete_disease_type"
                                        string="Supprimer tous les types de maladies"
                                        icon="gtk-apply"
                                        type="object"
                                    />
                                </group>
                            </page>
                            <page string="from presta of the day">
                                <button name="reset" string="Reset" icon="gtk-apply" type="object"/>
                                <button name="check" string="Check" icon="gtk-apply" type="object"/>
                                <button name="last_check_entry_exit" string="Check ES" icon="gtk-apply" type="object"/>
                                <button name="merge_pod_dup" string="merge pod dup" icon="gtk-apply" type="object"/>
                                <button name="del_pod_doublon" string="del pod doublon" icon="gtk-apply" type="object"/>
                                <button name="reset_verified_pod_with_non_verified_presta" string="bug pod verified"
                                        icon="gtk-apply" type="object"/>
                            </page>
                            <page string="Generate CODA file">
                                <group col="2">
                                    <field name="parent_id"/>
                                    <field name="coda_date"/>
                                    <field name="amount"/>
                                    <field name="communication"/>
                                    <button name="generate_coda" string="Generate File" icon="gtk-apply" type="object"/>
                                </group>
                            </page>
                            <page string="Biller">
                                <button name="reset_biller" string="Reset" icon="gtk-apply" type="object"/>
                                <button name="reconcil_all" string="Reconcil" icon="gtk-apply" type="object"/>
                            </page>
                            <page string="PO reprise">
                                <button name="reprise_signaletic" string="Reprise" icon="gtk-apply" type="object"/>
                            </page>
                            <page string="Reminder">
                                <field name="reminder_journal_id"/>
                                <button name="update_comm_struct" string="Update Comm Struct" icon="gtk-apply"
                                        type="object"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_extraschool_mainsettings">
            <field name="name">Main Settings</field>
            <field name="res_model">extraschool.mainsettings</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">inline</field>
            <field name="res_id">1</field>
            <field name="domain">[('id', '=', 1)]</field>
        </record>
    </data>
</odoo>
