<?xml version="1.0" ?>
<openerp>
	<data>
        <template id="tpl_guardian_horaire_wizard_report">

		</template>

		<record id="paperformat_aes_horaire_guardian" model="report.paperformat">
    		<field name="name">Horaire Guardian</field>
    		<field name="default" eval="True"/>
   			<field name="format">A4</field>
    		<field name="orientation">Portrait</field>
    		<field name="margin_top">1</field>
    		<field name="margin_bottom">1</field>
    		<field name="margin_left">3</field>
    		<field name="margin_right">3</field>
    		<field name="header_line" eval="False"/>
    		<field name="header_spacing">3</field>
    		<field name="dpi">80</field>
		</record>

		<report
		    id="horaire_guardian_wizard_report"
		    model="extraschool.horaire_guardian_wizard"
		    string="Horaire Guardian"
		    report_type="qweb-pdf"
		    name="extraschool.tpl_guardian_horaire_wizard_report"
		    attachment_use="False"
		/>

		<record id="qrcodes_wizard_report" model="ir.actions.report.xml">
    		<field name="paperformat_id" ref="paperformat_aes_horaire_guardian"/>
		</record>

	</data>
</openerp>
